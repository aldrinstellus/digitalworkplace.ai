{
  "name": "digitalworkplace-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:main": "turbo dev --filter=@digitalworkplace/main",
    "dev:intranet": "turbo dev --filter=@digitalworkplace/intranet-iq",
    "build": "turbo build",
    "build:main": "turbo build --filter=@digitalworkplace/main",
    "build:intranet": "turbo build --filter=@digitalworkplace/intranet-iq",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=18"
  }
}
